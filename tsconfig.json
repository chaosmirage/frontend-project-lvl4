{
  "compilerOptions": {
      "baseUrl": "./src",
      "outDir": "./dist/",
      "sourceMap": true,
      "strict": true,
      "noImplicitReturns": true,
      "noImplicitAny": true,
      "module": "es6",
      "moduleResolution": "node",
      "target": "esnext",
      "allowJs": false,
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "types": ["node"],
  },
  "include": [
      "./src/**/*"
  ]
}
